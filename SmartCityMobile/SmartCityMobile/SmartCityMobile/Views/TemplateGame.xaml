<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SmartCityMobile.Views.TemplateGame">
  <ViewCell.View>
        <StackLayout HorizontalOptions="StartAndExpand" HeightRequest="90" Orientation="Horizontal">
            <Label Text="{Binding ID}" HeightRequest="90" HorizontalOptions="StartAndExpand"/>
            <Label Text="{Binding SCOREFINAL}" HeightRequest="90" HorizontalOptions="StartAndExpand"/>
        </StackLayout>
  </ViewCell.View>
</ViewCell>